[user]
	name = No Name
	email = no@email.com
[alias]
	co =checkout
	st = status
	ci = commit
	rt = reset
	br = branch
[color]
	branch=auto
	diff=auto
	status=auto
[color "branch"]
	current=yellow reverse
	local=yellow
	remote=green
[color "diff"]
	meta=yellow bold
	frag=magenta bold
	old=red bold
	new=green bold
[color "status"]
	added=yellow
	changed=green
	untracked=cyan
[branch]
	autosetupmerge=true

#add gitignore with
#git config --global core.excludesfile ~/.g
[core]
	excludesfile = ~/.gitignore
